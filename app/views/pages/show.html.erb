

<%= link_to("<< Back to list",{:action => 'index'}, :class => 'back-link')%>

<div class="pages show">
  <h2>Show Pages</h2>
  <table summary="Page detail view">
    <tr>
      <th>Subject ID</th>
      <td><%= @page.subject_id %></th>
    </tr>
    <tr>
      <th>Name</th>
      <td><%= @page.name %></th>
    </tr>
    <tr>
      <th>Permalink</th>
      <td><%= @page.permalink %></th>
    </tr>
    <tr>
      <th>Position</th>
      <td><%= @page.position %></th>
    </tr>
    <tr>
      <th>visible</th>
      <td><%= @page.visible ? "Yes" : "No" %></th>
    </tr>
    <tr>
      <th>Created</th>
      <td><%= @page.created_at %></th>
    </tr>
  </table>
</div>
