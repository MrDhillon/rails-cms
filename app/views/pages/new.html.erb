<%= link_to("Back to index", {:action => 'index'}, :class => 'back-link') %>

<div class="page new">
  <h2>Create Section</h2>
  <%= form_for(:page, :url => {:action => 'create'}) do |f| %>
    <%= render(:partial => "form", :locals => {:f => f}) %>
    <div class="form-buttons">
      <%= submit_tag("Create Page")%>
    </div>
  <% end %>
</div>
