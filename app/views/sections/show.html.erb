

<%= link_to("<< Back to list",{:action => 'index'}, :class => 'back-link')%>

<div class="sections show">
  <h2>Show Sections</h2>
  <table summary="Section detail view">
    <tr>
      <th>Name</th>
      <td><%= @section.name %></th>
    </tr>
    <tr>
      <th>Position</th>
      <td><%= @section.position %></th>
    </tr>
    <tr>
      <th>visible</th>
      <td><%= @section.visible ? "Yes" : "No" %></th>
    </tr>
    <tr>
      <th>Created</th>
      <td><%= @section.created_at %></th>
    </tr>
    <tr>
      <th>content</th>
      <td><%= @section.content %></th>
    </tr>
  </table>
</div>
